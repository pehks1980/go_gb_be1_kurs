<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../part/head'); %>
</head>
<body class="container">

<header>
    <%- include('../part/header'); %>
</header>

<main>
    <div class="jumbotron">
        <h1>This is great</h1>
        <p>Welcome to templating using EJS</p>
        <ul>
        <% mascots.forEach(function(mascot) { %>
            <li>
                <strong><%= mascot.name %></strong>
                representing <%= mascot.organization %>, born <%= mascot.birth_year %>
            </li>
        <% }); %>
            <p></p>
            <li>
            <%= tagline %>
            </li>
            <li>
                <%= user.role %>
            </li>
        </ul>
    </div>
</main>

<footer>
    <%- include('../part/footer'); %>
</footer>

</body>
</html>
